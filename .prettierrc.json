{
  "plugins": ["@prettier/plugin-pug"],
  "trailingComma": "es5",
  "tabWidth": 2,
  "semi": true,
  "singleQuote": true,
  "pugSingleQuote": false,
  "pugPrintWidth": 80,
  "pugWrapAttributesThreshold": 3,
  "overrides": [
    {
      "files": "*.scss",
      "options": {
        "singleQuote": false
      }
    }
  ]
}
