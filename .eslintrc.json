{
  "parser": "@babel/eslint-parser",
  "extends": ["eslint:recommended", "airbnb-base", "prettier"],
  "env": {
    "browser": true,
    "es6": true
  },
  "parserOptions": {
    "ecmaVersion": 6
  },
  "rules": {
    "import/no-extraneous-dependencies": ["error", { "devDependencies": true }],
    "no-console": "off",
    "no-param-reassign": ["error", { "props": false }]
  }
}
